<Window x:Class="XcelGui.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="300" Width="300">
    <Grid>
        <GroupBox Header="Data" Margin="7,30,7,40" Name="groupBox1">
            <TextBox Height="23" Margin="10,10,10,10" Name="dataTextBox" />
        </GroupBox>
        <TextBox Height="23" Margin="77,0,86,7" Name="textBox2" VerticalAlignment="Bottom" />
        <ComboBox Margin="7,0,0,7" Name="comboBox1" HorizontalAlignment="Left" Width="65" Height="23" VerticalAlignment="Bottom" />
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,7,7" Name="button1" VerticalAlignment="Bottom" Width="75">Button</Button>
        <Menu Height="22" Margin="-1,0,0,0" Name="menu1" VerticalAlignment="Top">
            <MenuItem Header="Option">
                <MenuItem Header="New" />
                <MenuItem Header="Close" />
            </MenuItem>
        </Menu>
    </Grid>
</Window>
