<Window x:Class="TerryAndMike.XcelGui.XcelWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="XcelWindow" Height="174" Width="300" MinHeight="174" MinWidth="200" Icon="XcelGui.ico">
    <Grid>
        <GroupBox Header="Data" Margin="7,30,7,0" Name="groupBox1" Height="70" VerticalAlignment="Top">
            <TextBox Height="23" Margin="10,10,10,10" Name="dataTextBox" TextChanged="dataTextBox_TextChanged"></TextBox>
        </GroupBox>
        <TextBox Height="23" Margin="79,0,43,7" Name="outputTextBox" VerticalAlignment="Bottom" />
        <ComboBox Margin="7,0,0,7" Name="commandComboBox" HorizontalAlignment="Left" Width="66" Height="23" VerticalAlignment="Bottom" SelectionChanged="commandComboBox_SelectionChanged" />
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,7,7" Name="calcButton" VerticalAlignment="Bottom" Width="30" Click="Calc_Click">calc</Button>
        <Menu Height="22" Margin="-1,0,0,0" Name="menu1" VerticalAlignment="Top">
            <MenuItem Header="Option">
                <MenuItem Header="New" Click="MenuItem_Click"/>
                <MenuItem Header="Exit" Click="MenuItem_Click" />
            </MenuItem>
        </Menu>
    </Grid>
</Window>
